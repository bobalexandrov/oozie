</coordinator>
<coordinator name="coordOracleTable">
  <app-path>${coordroot}/coordoracletable.xml</app-path>
</coordinator>
<coordinator name="coordHiveToOracle">
  <app-path>${coordroot}/coordhivetooracle.xml</app-path>
</coordinator>
</bundle-app>
