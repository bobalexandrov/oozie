<datasets>
<dataset name="hivetable" frequency="${coord:days(1)}"
	initial-instance="${logsInitialInstance}" timezone="GMT">
	<uri-template>${flag_root}/hivetable/${YEAR}-${MONTH}-${DAY}</uri-template>
	<done-flag>_SUCCESS</done-flag>
</dataset>
<dataset name="oracletable" frequency="${coord:days(1)}"
	initial-instance="${logsInitialInstance}" timezone="GMT">
	<uri-template>${flag_root}/oracletable/${YEAR}-${MONTH}-${DAY}</uri-template>
	<done-flag>_SUCCESS</done-flag>
</dataset>
<dataset name="hivetooracle" frequency="${coord:days(1)}"
	initial-instance="${logsInitialInstance}" timezone="GMT">
	<uri-template>${flag_root}/hivetooracle/${YEAR}-${MONTH}-${DAY}</uri-template>
	<done-flag>_SUCCESS</done-flag>
</dataset>
<dataset name="root" frequency="${coord:days(1)}"
        initial-instance="${logsInitialInstance}" timezone="GMT">
        <uri-template>${flag_root}/${YEAR}-${MONTH}-${DAY}</uri-template>
        <done-flag>_SUCCESS</done-flag>
</dataset>

</datasets>
